#!/bin/sh
#
# PX4 FMUv6C specific board sensors init
#------------------------------------------------------------------------------
board_adc start

# Internal SPI bus ICM-20602
icm20602 -s -R 4 start

# Internal SPI bus ICM-20689
# icm20689 -s -R 2 start

# Internal SPI bus BMI055 accel/gyro
# bmi055 -A -R 2 -s start
# bmi055 -G -R 2 -s start

# internal compass
# hmc5883 -T -I -R 12 start
qmc5883l -I -f 1000 -R 4 start

# Internal baro
dps310 -I -f 1000 start -a 119
