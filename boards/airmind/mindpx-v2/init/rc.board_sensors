#!/bin/sh
#
# Airmind Mindpx-v2 specific board sensors init
#------------------------------------------------------------------------------

board_adc start

# Internal I2C bus
hmc5883 -T -I -R 2 start
qmc5883l -I -R 6 start

mpu6000 -s -R 2 start
mpu6500 -s -R 2 start
lsm303d -s -R 10 start
l3gd20 -s -R 14 start

# Internal SPI
ms5611 -s start
