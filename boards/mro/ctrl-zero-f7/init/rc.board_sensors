#!/bin/sh
#
# mRo Control Zero specific board sensors init
#------------------------------------------------------------------------------

# Internal ICM-20602
mpu6000 -R 10 -s -T 20602 start

# Internal ICM-20689
#icm20689 -R 10 20689 start

# Internal BMI088
bmi088 start

# Interal DPS310 (barometer)
dps310 start

# Possible external compasses
#ist8310 -C -b 1 start
#ist8310 -C -b 2 start
#hmc5883 -C -T -X start
#qmc5883 -X start
