#!/bin/sh
#
# Holybro KakuteH7v1 specific board sensors init
#------------------------------------------------------------------------------
board_adc start

# The KakuteH7 v1 comes by default with an ICM20689,
# but there might also be an MPU6000
if ! mpu6000 -R 6 -s start
then
	icm42688p -R 6 -s start
fi

bmp280 -X start
