#!/bin/sh
#
# intel aero specific board sensors init
#------------------------------------------------------------------------------

aerofc_adc start

if ! ms5611 -T 5607 start
then
	ms5611 start
fi
mpu9250 -s -R 14 start

# Possible external compasses
hmc5883 -X start

ist8310 -I -R 4 start

ll40ls start -X

# Internal SPI (auto detect ms5611 or ms5607)
if ! ms5611 -T 5607 -s start
then
	ms5611 -s start
fi
