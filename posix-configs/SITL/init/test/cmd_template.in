#!/bin/sh
# PX4 commands need the 'px4-' prefix in bash.
# (px4-alias.sh is expected to be in the PATH)
. px4-alias.sh

uorb start

param load
param set SYS_RESTART_TYPE 0

dataman start

simulator start -t
tone_alarm start
gyrosim start
accelsim start
barosim start
gpssim start
measairspeedsim start
pwm_out_sim start

ver all

mavlink start -x -u 14556 -r 2000000
mavlink boot_complete

@cmd_name@ start
sleep 3
@cmd_name@ start
sleep 1
@cmd_name@ stop

dataman status

shutdown
