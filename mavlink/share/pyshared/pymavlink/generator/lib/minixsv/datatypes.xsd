  <xs:simpleType name="unsignedByte" id="unsignedByte" facetType="xs:decimal">
    <xs:annotation>
      <xs:documentation
        source="http://www.w3.org/TR/xmlschema-2/#unsignedByte"/>
    </xs:annotation>
    <xs:restriction base="xs:unsignedShort">
